<?xml version="1.0" encoding="utf-8"?>
<Container>
	<TemplateURL>https://raw.githubusercontent.com/helixious/docker-templates/master/nodejs_playground.xml</TemplateURL>
	<Beta>False</Beta>
	<Name>EOS Node</Name>
	<Description>EOS Node Docker Container</Description>
	<Registry>https://registry.hub.docker.com/r/helixious86/eos_base_image/</Registry>
	<GitHub>https://github.com/helixious/eos_base_iamge/</GitHub>
	<Repository>helixious86/eos_base_image</Repository>
	<BindTime>true</BindTime>
	<Privileged>false</Privileged>
	<Environment>
		<Variable>
			<Value></Value>
			<Name>PIVKEY</Name>
			<Mode/>
		</Variable>
		<Variable>
			<Value></Value>
			<Name>PUBKEY</Name>
			<Mode/>
		</Variable>
		<Environment/>
		<Config Type="Path" Name="Data Directory Path" Target="~/biosboot" Default="/mnt/user/appdata/eos" Mode="rw" Description="This is where EOS will store it's blockchain data" Required="true">/mnt/user/appdata/eos
		</Config>

		<Networking>
			<Mode>bridge</Mode>
			<Publish>
				<Port>
					<HostPort>8888</HostPort>
					<ContainerPort>8888</ContainerPort>
					<Protocol>tcp</Protocol>
				</Port>
				<Port>
					<HostPort>2222</HostPort>
					<ContainerPort>22</ContainerPort>
					<Protocol>tcp</Protocol>
				</Port>
			</Publish>
		</Networking>
		<Icon>https://raw.githubusercontent.com/helixious/docker-templates/master/helixious86/images/eos-logo.png</Icon>
		<ExtraParams>-ti</ExtraParams>
	</Container>